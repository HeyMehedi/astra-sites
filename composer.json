{
    "name": "brainstormforce/astra-sites",
    "description": "[![pipeline status](https://github.com/brainstormforce/astra-sites/badges/master/pipeline.svg)](https://github.com/brainstormforce/astra-sites/commits/master)",  
    "require": {
        "brainstormforce/astra-notices": "^1.1",
        "brainstormforce/gutenberg-templates": "^1"
      },
    "require-dev": {
        "dealerdirect/phpcodesniffer-composer-installer": "0.7.0",
        "wp-coding-standards/wpcs": "dev-master",
        "phpcompatibility/phpcompatibility-wp": "*"
    },
    "extra": {
        "installer-paths": {
            "inc/lib/{$name}/": [
                "brainstormforce/astra-notices",
                "brainstormforce/gutenberg-templates"
            ]
        }
    },
    "repositories": [
		{
            "name": "brainstormforce/gutenberg-templates",
            "type": "vcs",
            "url": "git@github.com:brainstormforce/gutenberg-templates.git"
        }
	],
    "scripts": {
        "format": "phpcbf",
        "lint": "phpcs"
    }
}