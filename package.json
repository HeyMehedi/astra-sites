{
  "name": "astra-sites",
  "version": "2.6.17",
  "main": "Gruntfile.js",
  "author": "Brainstorm Force",
  "description": "[![pipeline status](https://github.com/brainstormforce/astra-sites/badges/master/pipeline.svg)](https://github.com/brainstormforce/astra-sites/commits/master)",
  "scripts": {
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "format:js": "wp-scripts format-js",
    "lint:css": "wp-scripts lint-style",
    "lint:js": "wp-scripts lint-js",
    "lint:md:docs": "wp-scripts lint-md-docs",
    "lint:md:js": "wp-scripts lint-md-js",
    "lint:pkg-json": "wp-scripts lint-pkg-json",
    "test:unit": "wp-scripts test-unit-js",
    "package": "rm -rf astra-sites && npm run build && grunt release-depracated",
    "release": "rm -rf astra-sites && npm run build && grunt release",
    "build": "wp-scripts build --entry=./inc/assets/js/src/ --output-path=./inc/assets/js/dist",
    "start": "wp-scripts start --entry=./inc/assets/js/src/ --output-path=./inc/assets/js/dist"
  },
  "keywords": [
    "reactjs",
    "components"
  ],
  "license": "ISC",
  "devDependencies": {
    "@wordpress/scripts": "^14.1.0",
    "grunt": "^1.3.0",
    "grunt-bumpup": "^0.6.3",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-compress": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-rtlcss": "^2.0.2",
    "grunt-text-replace": "^0.4.0",
    "grunt-wp-i18n": "^1.0.3",
    "webpack-cli": "^4.6.0",
    "grunt-wp-readme-to-markdown": "^2.0.1"
  },
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
